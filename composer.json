{
    "name": "4-check/backend",
    "description": "The 4-check backend is a RESTful web service implemented using the Lumen micro-framework.",
    "homepage": "https://4-check.com",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.6.*",
        "vlucas/phpdotenv": "~2.2",
        "ramsey/uuid": "^3.7",
        "barryvdh/laravel-cors": "^0.11.0",
        "laravel/passport": "5.0.3",
        "dusterio/lumen-passport": "^0.2.6",
        "illuminate/mail": "^5.6",
        "illuminate/support": "^5.6",
        "laravel/cashier": "~7.0",
        "simshaun/recurr": "^3.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0",
        "flipbox/lumen-generator": "^5.6",
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
